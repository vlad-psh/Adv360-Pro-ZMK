  compatible = "zmk,behavior-macro";
  label = "parentheses_pair";
  #binding-cells = <0>;
  bindings = <&kp LPAR>, <&kp RPAR>, <&kp LEFT>;
};
BKTPAIR: brackets_pair {
  compatible = "zmk,behavior-macro";
  label = "brackets_pair";
