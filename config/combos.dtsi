compatible = "zmk,combos";
combo_backspace {
    timeout-ms = <50>;
    key-positions = <69 70>; // RThumb: mod{numbers}+shift
    bindings = <&arr_bspc>;
};
combo_enter {
    timeout-ms = <50>;
    key-positions = <65 66>; // LThumb: ctrl+space
    bindings = <&kp ENTER>;
};
combo_mo_arrow {
    timeout-ms = <50>;
    slow-release;
    key-positions = <65 69>; // CTRL+{numbers}
    bindings = <&mo 3>;
};
